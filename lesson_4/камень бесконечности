    <script src="https://www.gstatic.com/firebasejs/5.8.2/firebase-app.js"></script>
<script src="https://www.gstatic.com/firebasejs/5.8.2/firebase-messaging.js"></script>
<script src="https://www.gstatic.com/firebasejs/5.8.2/firebase-auth.js"></script>
<script>
    providers = {
        'facebook.com': new firebase.auth.FacebookAuthProvider().addScope('email'),
        'google.com': new firebase.auth.GoogleAuthProvider(),
        'twitter.com': new firebase.auth.TwitterAuthProvider(),
    };

    firebase.initializeApp({
        apiKey: "AIzaSyCP3HLYnj2-8qhu89O92FOm2h9LICI6Iuo",
        authDomain: "goodfonweb.firebaseapp.com",
        projectId: "goodfonweb",
        messagingSenderId: "81648000881",
    });

    var doBackAuth = function() {
        return new Promise(function(resolve, reject) {
            firebase.auth().currentUser.getIdToken(true).then(function(idToken) {
                $.post('/auth/oauth/', {'token': idToken}).done(function() {
                    resolve()
                }).fail(function(e) {
                    reject(e);
                });
            });
        })
    }

    var doAuth = function(provider) {
        return new Promise(function(resolve, reject) {
            firebase.auth().signInWithPopup(provider).then(function () {
                doBackAuth().then(function () {
                   resolve()
                });
            }).catch(function (error) {
                if (error.code == 'auth/account-exists-with-different-credential') {
                    $.post('/auth/oauth_credential/', {'email': error.email}).done(function (res) {
                        firebase.auth().signInWithPopup(providers[res.result]).then(function (result) {
                            result.user.linkWithPopup(provider).then(function () {
                                doBackAuth().then(function () {
                                    resolve()
                                });
                            });
                        });
                    });
                }
            });
        });
    }

    var authOrLink = function(provider) {
        return new Promise(function(resolve, reject) {
            var user = firebase.auth().currentUser;

            if (user) {
                for(var i = 0; i < user.providerData.length; i++) {
                    if (user.providerData[i].providerId == provider.providerId) {
                        doAuth(provider).then(function () {
                           resolve()
                        });
                        return;
                    }
                }

                user.linkWithPopup(provider).then(function () {
                    doBackAuth().then(function () {
                        resolve()
                    });
                });
            } else {
                doAuth(provider).then(function () {
                   resolve()
                });
            }
        });
    }

    var authUnlink = function(provider) {
        return new Promise(function(resolve, reject) {
            firebase.auth().onAuthStateChanged(function(user) {
                if (user) {
                    user.unlink(provider).then(function(res) {
                        doBackAuth().then(function () {
                            resolve();
                        });
                    }).catch(function(e) {
                        reject(e);
                    });
                }
            });
        });
    }
</script>
